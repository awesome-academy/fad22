<% provide :title, t(".home") %>
<div class="center jumbotron">
  <h1><%= t(".fastfood") %></h1>
  <%= link_to t(".signup"), signup_path,
    class: "btn btn-lg btn-primary" unless logged_in?%>
</div>
<% if logged_in? && current_user.normal? %>
  <div id="mybutton">
    <%= link_to "Feedback", new_suggestion_path, class: "feedback" %>
  </div>
<% end %>
